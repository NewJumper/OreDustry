{
  "multipart": [
    {
      "apply": {
        "model": "oredustry:block/heat_cable"
      }
    },
    {
      "apply": {
        "model": "oredustry:block/heat_cable_vertical",
        "uvlock": true,
        "x": 180
      },
      "when": {
        "down": "true"
      }
    },
    {
      "apply": {
        "model": "oredustry:block/heat_cable_vertical",
        "uvlock": true
      },
      "when": {
        "up": "true"
      }
    },
    {
      "apply": {
        "model": "oredustry:block/heat_cable_horizontal",
        "uvlock": true,
        "y": 90
      },
      "when": {
        "north": "true"
      }
    },
    {
      "apply": {
        "model": "oredustry:block/heat_cable_horizontal",
        "uvlock": true,
        "y": 270
      },
      "when": {
        "south": "true"
      }
    },
    {
      "apply": {
        "model": "oredustry:block/heat_cable_horizontal",
        "uvlock": true
      },
      "when": {
        "west": "true"
      }
    },
    {
      "apply": {
        "model": "oredustry:block/heat_cable_horizontal",
        "uvlock": true,
        "y": 180
      },
      "when": {
        "east": "true"
      }
    }
  ]
}